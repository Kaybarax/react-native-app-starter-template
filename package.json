{
  "name": "react-native-ts-app-starter-template",
  "version": "1.0.0",
  "private": false,
  "author": {
    "name": "Kevin Barasa",
    "github": "https://github.com/Kaybarax",
    "linkedin": "https://linkedin.com/in/kaybarax",
    "twitter": "https://twitter.com/Kaybarax"
  },
  "repository": "https://github.com/Kaybarax/react-native-app-starter-template",
  "packageManager": "pnpm@8.6.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"app/app-dev-scratch-pad/**/*.{js,jsx,ts,tsx}\" \"app/app-management/**/*.{js,jsx,ts,tsx}\" \"app/auth-and-security/**/*.{js,jsx,ts,tsx}\" \"app/controllers/**/*.{js,jsx,ts,tsx}\" \"app/routing-and-navigation/**/*.{js,jsx,ts,tsx}\" \"app/shared-components-and-modules/**/*.{js,jsx,ts,tsx}\" \"app/stores/**/*.{js,jsx,ts,tsx}\" \"app/theme/**/*.{js,jsx,ts,tsx}\" \"app/util/**/*.{js,jsx,ts,tsx}\" \"app/views/**/*.{js,jsx,ts,tsx}\"",
    "install": "pnpm install",
    "update": "pnpm update",
    "clean": "rm -rf node_modules && rm -rf pnpm-lock.yaml && pnpm install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-native-fontawesome": "^0.3.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "expo": "~50.0.0",
    "expo-status-bar": "~1.11.1",
    "expo-camera": "~14.0.3",
    "expo-sqlite": "~13.2.2",
    "expo-checkbox": "~2.7.0",
    "expo-linking": "~6.2.2",
    "expo-crypto": "~12.8.0",
    "expo-asset": "~9.0.2",
    "expo-file-system": "~16.0.5",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-dropdownalert": "^5.1.0",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-pager-view": "6.2.3",
    "react-native-ratings": "^8.1.0",
    "react-native-reanimated": "~3.6.1",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-tab-view": "^3.5.2",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-transform-optional-chaining": "^7.23.4",
    "@babel/preset-env": "^7.23.7",
    "@babel/runtime": "^7.23.7",
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.47",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
